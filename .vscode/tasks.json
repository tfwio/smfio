{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build: cli35 release",
      "type": "process",
      "command": "cmd.exe",
      "args": [
        "/c",
        "${workspaceFolder}\\Build-dotnet-cli35-AnyCPU-Release.bat"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "Build: cli35 debug",
      "type": "process",
      "command": "cmd.exe",
      "args": [
        "/c",
        "${workspaceFolder}\\Build-dotnet-cli35-AnyCPU-Debug.bat"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "Run: Release",
      "type": "process",
      "command": "cmd.exe",
      "options": {
        "cwd": "${workspaceFolder}\\build\\AnyCPU-Release"
      },
      "args": [
        "/c",
        "${workspaceFolder}\\build\\AnyCPU-Release\\smfio.view.exe"
      ],
      "problemMatcher": []
    }
  ]
}