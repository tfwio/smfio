  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build: cli35 release",
      "type": "process",
      "command": "cmd.exe",
      "args": [
        "/c",
        "${workspaceFolder}\\Build-dotnet-cli35-AnyCPU-Release.bat"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
